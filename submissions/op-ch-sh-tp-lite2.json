{
	"tp": {
	"name": "op-ch-sh",
		"op_source": {
			"triples": {
				"triple": [
					"?p rdfs:range ?A",
					"?p rdfs:domain ?B",
					"?r rdfs:domain ?A",
					"?r rdfs:range ?C"
				]
			}
		},
		"op_target": {			
			"triples": {
				"triple": [
                    "?p rdfs:range ?A",
					"?p rdfs:domain ?B",
					"?r rdfs:domain ?A",
					"?r rdfs:range ?C",
					"?q rdfs:domain ?B",
					"?q rdfs:range ?C"
				]
			}
		},
		"naming_transformation":
            {                
                "lex": 
                    {
                        "?q": ["?p", "?A", "?r"]
                    }
                ,
                "ntp": 
                    {
                        "?q": ["relates ?A to ?C", "?p ?C"]
                    }
            }
	}
}
