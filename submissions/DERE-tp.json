{
    "tp": {
        "name": "DERE1",
        "op_source": {
            "triples": {
                "triple": [
                    "?p rdf:type owl:ObjectProperty",
                    "?p rdfs:domain ?reif",
                    "?p rdfs:range ?A",
                    "?q rdf:type owl:ObjectProperty",                    
                    "?q rdfs:domain ?reif",
                    "?q rdfs:range ?B"
                ]
            },
            "filters": {
                "filter": [                    
                    "FILTER(?A != ?B)"
                ]
            }
        },
        "op_target": {
            "triples": {
                "triple": [
                    "?u rdf:type owl:ObjectProperty",
                    "?u rdfs:domain ?A",
                    "?u rdfs:range ?B",
                    "?v rdf:type owl:ObjectProperty",
                    "?v rdfs:domain ?B",
                    "?v rdfs:range ?A"
                ]
            }
        },
        "naming_transformation":
            {                
                "ntp": 
                    {
                        "?u": ["nominalize(?reif)"], "?v": ["passivize(?reif)"]
                    }
            },
                    "llm_sort": {
        "example_values": [
        {
        "p":"responds to",
        "q":"opposed by",
        "A":"Issue",
        "B":"Argument",
        "reif":"Position",
        "v":"is argument against",
        "u":"has argument against"
        },
        {
        "p":"has indicator value",
        "q":"has super indicator",
        "A":"Indicator value",
        "B":"Indicator",
        "reif":"Indicator",
        "v":"has indicator value of sub indicator",
        "u":"has super indicator"
        }
        ]
}
    }
}
